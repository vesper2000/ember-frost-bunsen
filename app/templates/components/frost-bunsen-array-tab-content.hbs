{{#if cellConfig.item.renderer}}
  {{component customRenderer
    bunsenId=(concat bunsenId '.' index)
    cellConfig=cellConfig
    label=label
    model=model.items
    onChange=onChange
    readOnly=readOnly
    store=store
  }}
{{else if (eq model.items.type "object")}}
  {{frost-bunsen-model-container
    bunsenId=(concat bunsenId '.' index)
    cellConfig=cellConfig.item
    errors=errors
    label=label
    model=model.items
    onChange=onChange
    readOnly=readOnly
    store=store
    value=value
  }}
{{else}}
  {{frost-bunsen-input-wrapper
    bunsenId=(concat bunsenId '.' index)
    cellConfig=cellConfig.item
    errorMessage=errorMessage
    label=label
    model=model.items
    onChange=onChange
    readOnly=readOnly
    store=store
    value=value
  }}
{{/if}}
